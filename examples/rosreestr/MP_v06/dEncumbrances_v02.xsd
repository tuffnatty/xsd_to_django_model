					<xs:documentation>Решение об изъятии земельного участка, жилого помещения</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="022006000000">
				<xs:annotation>
					<xs:documentation>Аренда (в том числе, субаренда)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="022007000000">
				<xs:annotation>
					<xs:documentation>Ипотека</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="022008000000">
